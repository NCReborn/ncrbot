function checkEnv() {
  const required = [
    'DISCORD_TOKEN',
    'CLIENT_ID',
    'CRASH_LOG_CHANNEL_ID',
    'LOG_SCAN_CHANNEL_ID'
  ];
  let missing = [];
  for (const key of required) {
    if (!process.env[key]) missing.push(key);
  }
  if (missing.length) {
    console.warn('[WARN] Missing required environment variables:', missing.join(', '));
  }
}
module.exports = { checkEnv };
